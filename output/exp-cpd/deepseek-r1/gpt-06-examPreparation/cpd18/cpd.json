[
    {
        "stochastic_function": "lambda: {-1:0.2,0:0.4,1:0.4}",
        "variable": "exam difficulty",
        "evidence": []
    },
    {
        "stochastic_function": "lambda study_decision: {1: 0.899999, 0: 0.100001, -1: 0.0} if study_decision == 'study more' else {1: 0.3, 0: 0.699999, -1: 0.000001}",
        "variable": "current knowledge",
        "evidence": [
            "study decision"
        ]
    },
    {
        "stochastic_function": "lambda study_decision: {1:0.799999,0:0.100001,-1:0.1} if study_decision == 'study less' else {1:0.4999995,0:0.4999995,-1:0.000001}",
        "variable": "health status",
        "evidence": [
            "study decision"
        ]
    },
    {
        "stochastic_function": "lambda current_knowledge, health_status, exam_difficulty: {1: max(round(0.7-0.1*((1-current_knowledge)+(1-health_status)+(exam_difficulty+1)),10),0),0: round(0.9-(0.7-0.1*((1-current_knowledge)+(1-health_status)+(exam_difficulty+1))),10),-1:0.1}",
        "variable": "exam score",
        "evidence": [
            "current knowledge",
            "health status",
            "exam difficulty"
        ]
    }
